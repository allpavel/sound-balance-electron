{
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"assist": {
		"actions": {
			"source": {
				"organizeImports": "on"
			}
		}
	},
	"linter": {
		"rules": {
			"suspicious": {
				"noConsole": "error"
			},
			"correctness": {
				"noUnusedImports": "error"
			}
		}
	}
}
